# -------------------------------------------------------------------
# This is the style file for converting OSM nodes
# into Garmin POIs.
# Any line prefaced with # is a comment and will not be applied.
# Each line is a rule, of the generalised format:
#  OSM_tag(s)_to_match  [Garmin_type_code resolution]
# See http://wiki.openstreetmap.org/wiki/Mkgmap/help/style_rules
# and http://wiki.openstreetmap.org/wiki/Mkgmap/help/Custom_styles
# for more information.

#------------------------------------------------------------------------------
# Places

# 0x0100-0x0500   City name (Point, fat, big)
# 0x0600-0x0A00   City name (Point, big)
# 0x0B00-0x0D00   City name (Point, small)

place=city & population > 999999 & name=*                       {set cityxx=yes} [0x0100 resolution 12 continue with_actions]
place=city & population > 799999 & cityxx!=yes & name=*         {set cityxx=yes} [0x0200 resolution 13 continue with_actions]
place=city & population > 399999 & cityxx!=yes & name=*         {set cityxx=yes} [0x0300 resolution 14 continue with_actions]
place=city & population > 299999 & cityxx!=yes & name=*         {set cityxx=yes} [0x0400 resolution 14 continue with_actions]
place=city & population > 199999 & cityxx!=yes & name=*         {set cityxx=yes} [0x0500 resolution 15 continue with_actions]
place=city & population >  99999 & cityxx!=yes & name=*         {set cityxx=yes} [0x0600 resolution 15 continue with_actions]
place=city & population >  49999 & cityxx!=yes & name=*         {set cityxx=yes} [0x0700 resolution 15 continue with_actions]
place=city & population >   9999 & cityxx!=yes & name=*         {set cityxx=yes} [0x0800 resolution 16 continue with_actions]

place=city & population >      0 & cityxx!=yes & name=*         {set cityxx=yes} [0x0800 resolution 17 continue with_actions]
place=city                       & cityxx!=yes & name=*         {set cityxx=yes} [0x0700 resolution 17 continue with_actions]

place=town & population >  69999 & cityxx!=yes  & name=*        {set cityxx=yes} [0x0600 resolution 16 continue with_actions]
place=town & population >  19999 & cityxx!=yes  & name=*        {set cityxx=yes} [0x0700 resolution 16 continue with_actions]
place=town & population >   9999 & cityxx!=yes  & name=*        {set cityxx=yes} [0x0800 resolution 17 continue with_actions]
place=town & population >   4999 & cityxx!=yes  & name=*        {set cityxx=yes} [0x0900 resolution 17 continue with_actions]
place=town & population >    999 & cityxx!=yes  & name=*        {set cityxx=yes} [0x0a00 resolution 18 continue with_actions]
place=town & population >      0 & cityxx!=yes  & name=*        {set cityxx=yes} [0x0b00 resolution 18 continue with_actions]
place=town                       & cityxx!=yes  & name=*        {set cityxx=yes} [0x0800 resolution 18 continue with_actions]

place=village                    & cityxx!=yes  & name=*        {set cityxx=yes} [0x0900 resolution 19 continue with_actions]
place=suburb                     & cityxx!=yes  & name=*        {set cityxx=yes} [0x0a00 resolution 21 continue with_actions]

(place=hamlet |
place=isolated_dwelling |
place=neighbourhood)             & cityxx!=yes  & name=*        {set cityxx=yes} [0x0b00 resolution 22 continue with_actions]

place=island                     & cityxx!=yes  & name=*        {set cityxx=yes} [0x650c resolution 21 continue with_actions]
place=islet                      & cityxx!=yes  & name=*        {set cityxx=yes} [0x650c resolution 22 continue with_actions]

place=locality  & name=*                                                          [0x2800 resolution 24 continue with_actions]
# 0x2800-0x283F   Region name (no Point, small)

# make key/value usable, if values are separated with ';'
include '../inc/filter';

barrier=no {delete barrier}


# replace or delete some Key/value pairs to prevent listing in POI list
leisure=swimming_pool { deletealltags }
amenity=swimming_pool { deletealltags }

# Man Made
# 0x6401 	# bridge
amenity=shelter 							[0x6402 resolution 24 continue]
# 0x6402	# skyscaper
# 0x6403	# tombstone
amenity=grave_yard & mkgmap:area2poi!=true				[0x6404 resolution 24]
landuse=cemetery & mkgmap:area2poi!=true				[0x6404 resolution 24]
# 0x6405	# civil

# 0x6406	# crossing
railway=level_crossing | railway=crossing
            {
              set mkgmap:road-speed=0
              }                                                         [0x6406 resolution 24]
# 0x6407	# dam
# 0x6408	# hospital
# 0x6409	# levee
# 0x640a	# locale
military=bunker 							[0x640b resolution 24]
man_made=mineshaft 					                [0x640c resolution 24]
man_made=petroleum_well 						[0x640d	resolution 24]
# 0x640e	# park

amenity=post_box							[0x640f resolution 24]

amenity=clock 								[0x6410 resolution 24]

(man_made=mast | man_made=tower)
              & tower:type=communication                                [0x5103 resolution 24]
man_made=tower & tower:type=observation                                 [0x6600 resolution 24]

man_made=tower & tower:type=bell_tower                                  [0x5b0c resolution 24]
man_made=tower 								[0x6411 resolution 24]

(tourism=information
          & information=guidepost) |
            amenity=guidepost |
            amenity=signpost                                            [0x6412 resolution 24]
man_made=adit                                                           [0x6413 resolution 24]

(amenity=drinking_water & (drinking_water=yes|drinkable=yes)) |
        (amenity=water_well & water_well=drinking_water)		[0x6414 resolution 24]
man_made=water_tower 							[0x6415 resolution 24]
amenity=hunting_stand 							[0x6417 resolution 24]
man_made=survey_point 							[0x6418 resolution 24]
man_made=flagpole 							[0x6419 resolution 24]
man_made=reservoir_covered  & mkgmap:area2poi!=true   			[0x641a resolution 24]
man_made=observatory | man_made=telescope | landuse=observatory         [0x641b resolution 24]
man_made=water_mill  & mkgmap:area2poi!=true                            [0x641c resolution 24]
man_made=gasometer | man_made=storage_tank & mkgmap:area2poi!=true      [0x641d resolution 24]
man_made=works & mkgmap:area2poi!=true                                  [0x641e resolution 24]
man_made=windmill & mkgmap:area2poi!=true                               [0x641f resolution 24]

