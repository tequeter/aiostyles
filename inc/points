man_made=observatory | man_made=telescope | landuse=observatory         [0x641b resolution 24]
man_made=water_mill  & mkgmap:area2poi!=true                            [0x641c resolution 24]
man_made=gasometer | man_made=storage_tank & mkgmap:area2poi!=true      [0x641d resolution 24]
man_made=works & mkgmap:area2poi!=true                                  [0x641e resolution 24]
man_made=windmill & mkgmap:area2poi!=true                               [0x641f resolution 24]

